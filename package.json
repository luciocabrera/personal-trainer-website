{
  "name": "personal-trainer",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer build/client",
    "build:production": "NODE_ENV=production npm run build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "start:express": "node server-express.js",
    "start:rr": "react-router-serve ./build/server/index.js",
    "lint": "eslint .",
    "preview": "npm run build && npm run start",
    "knip": "knip",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix",
    "check:unused": "knip --max-issues 1",
    "check:unused:fix": "knip --fix",
    "typegen": "react-router typegen",
    "clean": "rm -rf build node_modules/.vite",
    "build:clean": "npm run clean && npm run build"
  },
  "dependencies": {
    "@react-router/node": "^7.9.4",
    "@react-router/serve": "^7.9.4",
    "@stylexjs/stylex": "^0.16.2",
    "@types/nodemailer": "^7.0.2",
    "@types/pg": "^8.15.5",
    "i18next": "^25.6.0",
    "nodemailer": "^7.0.9",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.1.0",
    "react-router": "^7.9.4",
    "resend": "^6.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@react-router/dev": "^7.9.4",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "knip": "^5.66.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-stylex": "^0.13.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
